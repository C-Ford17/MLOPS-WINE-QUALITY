name: winequality-classification

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: null}
      class_weight: {type: str, default: balanced}
    command: >
      python train.py --n_estimators {n_estimators} --max_depth {max_depth} --class_weight {class_weight}
